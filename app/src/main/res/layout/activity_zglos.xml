<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tlo_zglos"
    tools:context=".Zglos">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="301dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:transitionName="aparat123"
        app:srcCompat="@drawable/aparat" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="130dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="130dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Zgłoś"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/zglos1"
        android:layout_width="385dp"
        android:layout_height="252dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="12dp"
        app:srcCompat="@drawable/zglos1" />

    <ImageView
        android:id="@+id/zglos2"
        android:layout_width="385dp"
        android:layout_height="252dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="12dp"
        app:srcCompat="@drawable/zglos2" />

    <ImageView
        android:id="@+id/zglos3"
        android:layout_width="385dp"
        android:layout_height="252dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="359dp"
        android:layout_marginEnd="12dp"
        app:srcCompat="@drawable/zglos3" />

    <Button
        android:id="@+id/zaczynamy"
        android:layout_width="wrap_content"
        android:layout_height="99dp"
        android:layout_alignBottom="@+id/zglos2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="-206dp"
        android:background="@drawable/zaczynamy" />


</RelativeLayout>